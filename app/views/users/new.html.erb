<%= form_for :users do |f| %>
  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :Email%><br>
    <%= f.text_area :email %>
  </p>

  <p>
    <%= f.label :Password %><br>
    <%= f.text_area :password %>
  </p>
 
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
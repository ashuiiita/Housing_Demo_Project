<%= form_for :post, url: posts_path do |f| %>
  <p>
    <%= f.label :content %><br>
    <%= f.text_field :content %>
  </p>
 
  <p>
    <%= f.label :token %><br>
    <%= f.text_area :token %>
  </p>
 
  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>
  <p>
    <%= f.label :latitude %><br>
    <%= f.text_area :latitude %>
  </p>

  <p>
    <%= f.label :longitude %><br>
    <%= f.text_area :longitude %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>